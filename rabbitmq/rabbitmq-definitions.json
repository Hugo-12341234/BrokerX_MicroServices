{
  "rabbit_version": "3.13.0",
  "rabbitmq_version": "3.13.0",
  "product_name": "RabbitMQ",
  "product_version": "3.13.0",
  "exchanges": [
    {
      "name": "order.exchange",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "order.exchange.dlx",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "matching.exchange",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "notification.exchange",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "order.placed.queue",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "order.exchange.dlx"
      }
    },
    {
      "name": "order.placed.queue.dlq",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "order.matched.queue",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "order.rejected.queue",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "notification.send.queue",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "order.exchange",
      "destination": "order.placed.queue",
      "destination_type": "queue",
      "routing_key": "order.placed",
      "arguments": {}
    },
    {
      "source": "order.exchange.dlx",
      "destination": "order.placed.queue.dlq",
      "destination_type": "queue",
      "routing_key": "order.placed",
      "arguments": {}
    },
    {
      "source": "matching.exchange",
      "destination": "order.matched.queue",
      "destination_type": "queue",
      "routing_key": "order.matched",
      "arguments": {}
    },
    {
      "source": "matching.exchange",
      "destination": "order.rejected.queue",
      "destination_type": "queue",
      "routing_key": "order.rejected",
      "arguments": {}
    },
    {
      "source": "notification.exchange",
      "destination": "notification.send.queue",
      "destination_type": "queue",
      "routing_key": "notification.send",
      "arguments": {}
    }
  ],
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ]
}

